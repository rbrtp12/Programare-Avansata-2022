<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">

      <mapping-file>MembersEntity.xml</mapping-file>
      <mapping-file>ZodiacsEntity.xml</mapping-file>
      <class>View.MembersEntity</class>
      <class>View.ZodiacsEntity</class>
      <properties>
          <property name="hibernate.connection.url" value="jdbc:oracle:thin:@localhost:1521:XE"/>
          <property name="hibernate.connection.driver_class" value="oracle.jdbc.OracleDriver"/>
          <property name="hibernate.connection.username" value="student"/>
          <property name="hibernate.connection.password" value="student"/>
      </properties>
  </persistence-unit>
</persistence>
